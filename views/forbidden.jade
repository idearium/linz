extends layout-preauth

block content
	.pane.centered.login.col-md-6.col-sm-6.col-xs-12
		h1.title Forbidden.
		form
			p You don't have the permissions to view this page.
			p If you feel this is an error, please contact the administrator of this website.
			button.btn.btn-primary(type='submit') Back

block script
	script.
		$('form .btn-primary').click(function (event) {
			event.preventDefault();
			window.history.back();
		});
